{
  "name": "ganbarometer-beta",
  "version": "4.0.5",
  "description": "The GanbarOmeter (Wanikani dashboard widgets)",
  "author": "Rex Walters -- rw [at] pobox.com",
  "homepage": "https://github.com/wrex/ganbarometer-svelte#readme",
  "scripts": {
    "build": "tsc && rollup -c",
    "dev": "rollup -c -w",
    "test": "jest src",
    "test:watch": "npm run test -- --watch",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "publish": "tsc && rollup -c && cp ./dist/bundle.user.js ./published/v4/ && cp ./dist/bundle.css ./published/v4/",
    "publishdev": "tsc && rollup -c && cp ./dist/bundle.css ./published/dev/ && cat ./dist/bundle.user.js | sed 's:/published/v4/:/published/dev/:' > ./published/dev/bundle.user.js",
    "publishbeta": "tsc && rollup -c && cp ./dist/bundle.css ./published/v4beta/ && cat ./dist/bundle.user.js | sed 's:/published/v4/:/published/v4beta/:' > ./published/v4beta/bundle.user.js"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-typescript": "^7.15.0",
    "@mswjs/data": "^0.8.1",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@rollup/plugin-typescript": "^8.0.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/svelte": "^3.0.3",
    "@testing-library/user-event": "^13.5.0",
    "@tsconfig/svelte": "^2.0.1",
    "@types/jest": "^27.0.2",
    "@types/tampermonkey": "^4.0.0",
    "babel-jest": "^27.1.0",
    "fake-indexeddb": "^3.1.7",
    "faker": "^5.5.3",
    "jest": "^27.1.0",
    "magic-string": "^0.25.7",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.0",
    "rollup-plugin-userscript-metablock": "^0.3.1",
    "svelte": "^3.38.2",
    "svelte-check": "^2.1.0",
    "svelte-jester": "^2.1.5",
    "svelte-loading-spinners": "^0.1.7",
    "svelte-preprocess": "^4.7.3",
    "svelte-range-slider-pips": "^2.0.0",
    "tslib": "^2.0.0",
    "typescript": "^4.0.0"
  },
  "jest": {
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.svelte$": [
        "svelte-jester",
        {
          "preprocess": true
        }
      ]
    },
    "moduleFileExtensions": [
      "js",
      "ts",
      "svelte"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom/extend-expect"
    ],
    "setupFiles": [
      "fake-indexeddb/auto"
    ]
  },
  "dependencies": {
    "@okrad/svelte-progressbar": "^1.11.2",
    "mathjs": "^10.0.0",
    "vest": "^4.0.0-next-3118f5"
  }
}
